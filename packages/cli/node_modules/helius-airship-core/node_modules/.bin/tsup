else
  export NODE_PATH="/mnt/c/Users/Metabox/.cursor/airdrop 2/airship/node_modules/.pnpm/tsup@8.2.4_@microsoft+api-extractor@7.47.9_@types+node@22.13.4__jiti@1.21.6_postcss@8.4.44_typescript@5.5.4_yaml@2.7.0/node_modules/tsup/dist/node_modules:/mnt/c/Users/Metabox/.cursor/airdrop 2/airship/node_modules/.pnpm/tsup@8.2.4_@microsoft+api-extractor@7.47.9_@types+node@22.13.4__jiti@1.21.6_postcss@8.4.44_typescript@5.5.4_yaml@2.7.0/node_modules/tsup/node_modules:/mnt/c/Users/Metabox/.cursor/airdrop 2/airship/node_modules/.pnpm/tsup@8.2.4_@microsoft+api-extractor@7.47.9_@types+node@22.13.4__jiti@1.21.6_postcss@8.4.44_typescript@5.5.4_yaml@2.7.0/node_modules:/mnt/c/Users/Metabox/.cursor/airdrop 2/airship/node_modules/.pnpm/node_modules:$NODE_PATH"
